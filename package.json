{
  "name": "kpc",
  "version": "0.7.10",
  "description": "A component library written in intact for intact and vue",
  "browser": "@css/index.js",
  "main": "dist/kpc.js",
  "module": "index.js",
  "scripts": {
    "test": "karma start test/karma.conf.js",
    "test:react": "karma start test/karma.react.conf.js",
    "snapshot": "cross-env UPDATE=1 npm run test",
    "snapshot:prune": "cross-env UPDATE=3 npm run test",
    "cover": "cross-env UPDATE=2 npm run test",
    "start": "node server.js",
    "build": "cross-env NODE_ENV=production BUILD=1 gulp build",
    "dev:doc": "cross-env THEME=ksyun gulp watch",
    "dev:doc:default": "cross-env THEME=default gulp watch",
    "preview:doc": "gulp watch",
    "build:doc": "cross-env NODE_ENV=production gulp build:doc",
    "watch:doc": "cross-env NODE_ENV=production gulp watch:doc",
    "deploy:doc": "cross-env NODE_ENV=production gulp deploy:doc",
    "prepublishOnly": "npm run build && (git commit -am'chore: build' || echo 'has not files to commit') && git push --tags",
    "release": "npm run release-patch",
    "prelease": "npm version prerelease && npm publish",
    "release-patch": "npm version patch && npm publish",
    "release-minor": "npm version minor && npm publish",
    "release-major": "npm version major && npm publish"
  },
  "repository": {
    "type": "git",
